<launch>
  <param name="/use_sim_time" value="true" />
  <arg name="model" default="/home/jason/catkin_ws/src/turtlebot3/turtlebot3_description/urdf/turtlebot3_burger.urdf.xacro"/>
  <arg name="rvizconfig" default="$(find robot_localization)/rviz/odom_turtlebot3.rviz" />
  <arg name="bag_filename" default="/home/jason/download/bagfiles/test_local_1.bag"/>
  <rosparam command="load" file="$(find robot_localization)/params/ekf_template_test_2gps.yaml" />

  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_se_odom" clear_params="true" />

   <!--  Placeholder for output topic remapping
    <remap from="odometry/filtered" to=""/>
    <remap from="accel/filtered" to=""/>
    

    -->


  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_se_map" clear_params="true">
    <remap from="odometry/filtered" to="odometry/filtered_map"/>
  </node>


<!-- 1gps -->
  <node pkg="robot_localization" type="navsat_transform_node" name="navsat_transform0" clear_params="true" output="screen">
    <remap from="odometry/filtered" to="odometry/filtered_map" />
    <remap from="imu/data" to="imu" />
    <remap from="gps/fix" to="gps/fix1"/>
    <remap from="odometry/gps" to="odometry/gps1"/>
  </node>

  <node pkg="robot_localization" name="edit_gps_topic1" type="edit_gps_frame_id.py">
    <remap from="fix" to="fix1"/>
    <remap from="gps/fix" to="gps/fix1"/>
  </node>
  

<!-- 2gps -->

  <node pkg="robot_localization" type="navsat_transform_node" name="navsat_transform1" clear_params="true" output="screen">
    <remap from="odometry/filtered" to="odometry/filtered_map" />
    <remap from="imu/data" to="imu" />
    <remap from="gps/fix" to="gps/fix0"/>
    <remap from="odometry/gps" to="odometry/gps0"/>
  </node>

  <node pkg="robot_localization" name="edit_gps_topic0" type="edit_gps_frame_id.py">
    <remap from="fix" to="fix0"/>
    <remap from="gps/fix" to="gps/fix0"/>
  </node>


  <include file="$(find urdf_tutorial)/launch/display.launch">
    <arg name="model" value="$(arg model)" />
    <arg name="rvizconfig" value="$(arg rvizconfig)"/>
  </include>

  <node name="playbag" pkg="rosbag" type="play" args="--clock $(arg bag_filename)">

  </node>

</launch>
